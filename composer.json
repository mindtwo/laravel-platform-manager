{
    "name": "mindtwo/laravel-platform-manager",
    "description": "A pacakage to resolve platforms.",
    "keywords": [
        "domains",
        "multidomain",
        "multisite",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mindtwo/laravel-platform-manager",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://mindtwo.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "mindtwo/native-enum": "^1.7",
        "mindtwo/laravel-auto-create-uuid": "^2.5",
        "laravel/framework": "^9.52"
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "orchestra/testbench": "^7.24",
        "pestphp/pest": "^1.23",
        "pestphp/pest-plugin-laravel": "^1.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "mindtwo\\LaravelPlatformManager\\": "src/",
            "mindtwo\\LaravelPlatformManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\LaravelPlatformManager\\Providers\\LaravelPlatformManagerProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
