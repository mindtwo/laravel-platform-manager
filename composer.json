{
    "name": "mindtwo/laravel-platform-manager",
    "description": "A pacakage to resolve platforms.",
    "keywords": [
        "domains",
        "multidomain",
        "multisite",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mindtwo/laravel-platform-manager",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://mindtwo.de"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "mindtwo/native-enum": "^1.7",
        "mindtwo/laravel-auto-create-uuid": "^2.6",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.0|^2.0",
        "pestphp/pest-plugin-laravel": "^1.0|^2.0",
        "pestphp/pest-plugin-watch": "^1.0|^2.0",
        "laravel/nova": "^4.26",
        "nunomaduro/larastan": "^2.0",
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/promises": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "mindtwo\\LaravelPlatformManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mindtwo\\LaravelPlatformManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\LaravelPlatformManager\\Providers\\LaravelPlatformManagerProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/mindtwo/"
        }
    ]
}
